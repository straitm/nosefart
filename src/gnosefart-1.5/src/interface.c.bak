GtkWidget*
create_audio_error (void)
{
  GtkWidget *audio_error;
  GtkWidget *vbox3;
  GtkWidget *label20;
  GtkWidget *hbuttonbox3;
  GtkWidget *button4;

  audio_error = gtk_dialog_new ();
  gtk_window_set_title (GTK_WINDOW (audio_error), _("Can't play audio"));
  gtk_window_set_destroy_with_parent (GTK_WINDOW (audio_error), TRUE);
  gtk_window_set_type_hint (GTK_WINDOW (audio_error), GDK_WINDOW_TYPE_HINT_DIALOG);

  vbox3 = GTK_DIALOG (audio_error)->vbox;
  gtk_widget_show (vbox3);

  label20 = gtk_label_new (_("I can't seem to play music.  Please make sure that:\n\n1) You have your sound card properly configured \n2) No other program is using the sound card"));
  gtk_widget_show (label20);
  gtk_box_pack_start (GTK_BOX (vbox3), label20, FALSE, FALSE, 0);
  GTK_WIDGET_SET_FLAGS (label20, GTK_CAN_FOCUS);
  gtk_label_set_line_wrap (GTK_LABEL (label20), TRUE);
  gtk_label_set_selectable (GTK_LABEL (label20), TRUE);

  hbuttonbox3 = GTK_DIALOG (audio_error)->action_area;
  gtk_widget_show (hbuttonbox3);
  gtk_button_box_set_layout (GTK_BUTTON_BOX (hbuttonbox3), GTK_BUTTONBOX_END);

  button4 = gtk_button_new_from_stock ("gtk-close");
  gtk_widget_show (button4);
  gtk_dialog_add_action_widget (GTK_DIALOG (audio_error), button4, GTK_RESPONSE_CLOSE);
  GTK_WIDGET_SET_FLAGS (button4, GTK_CAN_DEFAULT);

  g_signal_connect ((gpointer) button4, "released",
                    G_CALLBACK (closebutton_pushed_in_audio_error_window),
                    NULL);
  g_signal_connect ((gpointer) button4, "activate",
                    G_CALLBACK (closebutton_pushed_in_audio_error_window),
                    NULL);

  /* Store pointers to all widgets, for use by lookup_widget(). */
  GLADE_HOOKUP_OBJECT_NO_REF (audio_error, audio_error, "audio_error");
  GLADE_HOOKUP_OBJECT_NO_REF (audio_error, vbox3, "vbox3");
  GLADE_HOOKUP_OBJECT (audio_error, label20, "label20");
  GLADE_HOOKUP_OBJECT_NO_REF (audio_error, hbuttonbox3, "hbuttonbox3");
  GLADE_HOOKUP_OBJECT (audio_error, button4, "button4");

  return audio_error;
}

