GtkWidget*
create_dialog2 (void)
{
  GtkWidget *dialog2;
  GtkWidget *dialog_vbox1;
  GtkWidget *scrolledwindow1;
  GtkWidget *textview2;
  GtkWidget *dialog_action_area1;
  GtkWidget *okbutton1;

  dialog2 = gtk_dialog_new ();
  gtk_widget_set_size_request (dialog2, 480, 480);
  gtk_window_set_title (GTK_WINDOW (dialog2), _("Gnosefart Help"));
  gtk_window_set_type_hint (GTK_WINDOW (dialog2), GDK_WINDOW_TYPE_HINT_DIALOG);

  dialog_vbox1 = GTK_DIALOG (dialog2)->vbox;
  gtk_widget_show (dialog_vbox1);

  scrolledwindow1 = gtk_scrolled_window_new (NULL, NULL);
  gtk_widget_show (scrolledwindow1);
  gtk_box_pack_start (GTK_BOX (dialog_vbox1), scrolledwindow1, TRUE, TRUE, 0);

  textview2 = gtk_text_view_new ();
  gtk_widget_show (textview2);
  gtk_container_add (GTK_CONTAINER (scrolledwindow1), textview2);
  gtk_text_view_set_editable (GTK_TEXT_VIEW (textview2), FALSE);
  gtk_text_view_set_wrap_mode (GTK_TEXT_VIEW (textview2), GTK_WRAP_WORD);
  gtk_text_view_set_cursor_visible (GTK_TEXT_VIEW (textview2), FALSE);
  gtk_text_buffer_set_text (gtk_text_view_get_buffer (GTK_TEXT_VIEW (textview2)), _("This is gnosefart 1.0, a front-end to nosefart.  You need to have nosefart version 2.0-mls or later for everything to work.  If you installed gnosefart in the obvious way, all this has been taken care of for you.\n\nNosefart is a NES (Nintendo Entertainment System) music player.  To use this program, you will need NSF (NES Sound Format) files, which can be found at emulation sites such as http://zophar.net.\n\nUsing this program:\n\nOpen: Opens a NSF file and starts playing it.\n\nClose: Stops playing the open file and closes it.\n\nPlay options: You have three options:\n- You can have the current track play forever (until you switch it).\n- You can have it play for a certain number of seconds and then move on to the next track.\n- You can have it play for a certain number of repetitions.  In this case nosefart will calculate the length of the song loop and play the specified number of repetitions.  Since many songs have a non-repeated introduction, \"0 repetitions\" means nosefart will just play the intro.\n\nSpeed: Self explanatory.  You can play the music faster or slower than normal.\n\nChannels: The NES uses 6 channels of sound.  The first two are square waves, usually used for the melody.  The third is a triangle wave, usually used for the bass line. The fourth is noise, used for drum beats, explosions, etc.  The fifth is \"PCM\" and the sixth is \"external\".\n\nIf you find a bug or have a feature request, please write Matthew Strait at <quadong@users.sf.net> or post to the nosefart project page at http://sf.net/projects/nosefart"), -1);

  dialog_action_area1 = GTK_DIALOG (dialog2)->action_area;
  gtk_widget_show (dialog_action_area1);
  gtk_button_box_set_layout (GTK_BUTTON_BOX (dialog_action_area1), GTK_BUTTONBOX_END);

  okbutton1 = gtk_button_new_from_stock ("gtk-ok");
  gtk_widget_show (okbutton1);
  gtk_dialog_add_action_widget (GTK_DIALOG (dialog2), okbutton1, GTK_RESPONSE_OK);
  GTK_WIDGET_SET_FLAGS (okbutton1, GTK_CAN_DEFAULT);

  g_signal_connect ((gpointer) okbutton1, "released",
                    G_CALLBACK (ok_released_in_help),
                    NULL);
  g_signal_connect ((gpointer) okbutton1, "activate",
                    G_CALLBACK (ok_released_in_help),
                    NULL);

  /* Store pointers to all widgets, for use by lookup_widget(). */
  GLADE_HOOKUP_OBJECT_NO_REF (dialog2, dialog2, "dialog2");
  GLADE_HOOKUP_OBJECT_NO_REF (dialog2, dialog_vbox1, "dialog_vbox1");
  GLADE_HOOKUP_OBJECT (dialog2, scrolledwindow1, "scrolledwindow1");
  GLADE_HOOKUP_OBJECT (dialog2, textview2, "textview2");
  GLADE_HOOKUP_OBJECT_NO_REF (dialog2, dialog_action_area1, "dialog_action_area1");
  GLADE_HOOKUP_OBJECT (dialog2, okbutton1, "okbutton1");

  return dialog2;
}

